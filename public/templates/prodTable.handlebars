{{#if prod}}
<table class="table">
        <thead>
          <tr>
            <th scope="col">ID</th>
            <th scope="col">NOMBRE</th>
            <th scope="col">DESCRIPCION</th>
            <th scope="col">PRECIO</th>
          </tr>
        </thead>
        <tbody>
        {{#each prod}}
          <tr>
            <th scope="row">{{this.id}}</th>
            <td>{{this.name}}</td>
            <td>{{this.descripcion}}</td>
            <td>{{this.precio}}</td>
          </tr>
        {{/each}}
        </tbody>
</table>
{{/if}}
