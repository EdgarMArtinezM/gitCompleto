<% let exist = food.length>0%>
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <%if(exist){%>
        <table>
            <tr>
                <th>ID</th>
                <th>NOMBRE</th>
                <th>DESCRIPCION</th>
                <th>PRECIO</th>
            </tr>
            <% food.forEach(function(f){%>
                <tr>
                    <th> <%=f.id%></th>
                    <th> <%=f.name%></th>
                    <th> <%=f.descripcion%></th>
                    <th><%=f.precio%></th>
                </tr>
        </table>
       

        <%}else{%>
            <p> No hay comida para mostrar</p>
        <%}%>
</body>
</html>